{
  "report-dir": "./.coverage",
  "exclude": [
    "build/src/*{/*,/**/*}.js",
    "build/src/*/v*/*.js",
    "build/test/**/*.js"
  ],
  "watermarks": {
    "branches": [
      95,
      100
    ],
    "functions": [
      95,
